  "scoring_guidelines": {
    "theta_interpretation": {
      "very_low": {
        "range": [-4.0, -1.5],
        "label": "Minimal/No Symptoms",
        "description": "Symptom level well below clinical threshold"
      },
      "low": {
        "range": [-1.5, -0.5],
        "label": "Mild Symptoms",
        "description": "Some symptoms present but below clinical concern"
      },
      "average": {
        "range": [-0.5, 0.5],
        "label": "Moderate Symptoms",
        "description": "Clinical attention warranted"
      },
      "high": {
        "range": [0.5, 1.5],
        "label": "Moderately Severe Symptoms",
        "description": "Active treatment recommended"
      },
      "very_high": {
        "range": [1.5, 4.0],
        "label": "Severe Symptoms",
        "description": "Intensive treatment indicated"
      }
    },
    "reliability_standards": {
      "excellent": "> 0.90",
      "good": "0.80 - 0.89",
